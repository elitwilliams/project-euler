def find_triplets(num):
    a = 1
    b = 1
    c = 2
    triplets = []
    while c < num:
        c + 1
        while b < c:
            b + 1
            while a < b:
                if a**2 + b**2 == c**2:
                    triplets.append([a,b,c])
                else:
                    a + 1
    return triplets

print find_triplets(10)
    
		

